{
  "extends": [
    "@open-wc/eslint-config",
    "eslint-config-prettier"
  ],
  "rules": {
      "quotes": ["error", "single"],
      "no-use-before-define": ["error", { "functions": false, "classes": false, "variables": true }],
      "lines-between-class-members": "off",
      "no-nested-ternary": "off"
  }
}